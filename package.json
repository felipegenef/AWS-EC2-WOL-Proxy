  "scripts": {
    "proxy": "node proxy.js",
    "cluster": "node proxyClusters.js",
    "http": "node handler.js",
    "test:Db": "docker-compose up -d && npx query.js",
    "test": "npx autocannon -c 1000 -d 30 --renderStatusCodes --workers 10  localhost:8080",
    "dev": "pm2 start proxy.js",
    "dev:cluster": "pm2 start proxyClusters.js",
    "start": "npx pm2 start proxy.js",
    "start:cluster": "npx pm2 start proxyClusters.js",
    "stop": "npx pm2 delete proxy",
    "stop:Db": "npx pm2 delete proxyClusters"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.110.0",
    "arg": "^5.0.2",
    "cli-progress": "^3.11.1",
